{
  "request": {
    "method": "GET",
    "urlPattern": "/api/plm/pipelines/([^/]+)$",
    "headers": {
      "Authorization": {
        "matches": "Bearer .*"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/yaml"
    },
    "bodyFileName": "pipelines/pipeline-def.yaml"
  }
}