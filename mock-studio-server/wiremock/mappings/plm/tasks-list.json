{
  "request": {
    "method": "GET",
    "urlPattern": "/api/plm/tasks.*",
    "headers": {
      "Authorization": {
        "matches": "Bearer .*"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "tasks/list.json"
  }
}