{
  "request": {
    "method": "POST",
    "url": "/api/auth/revoke",
    "headers": {
      "Authorization": {
        "matches": "Bearer .*"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "success": true,
      "message": "Token revoked successfully"
    }
  }
}